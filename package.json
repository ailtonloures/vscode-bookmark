{
	"name": "vscode-bookmark",
	"productName": "VSCode Bookmark",
	"version": "0.1.0",
	"description": "A utility for vscode developed with electron",
	"license": "MIT",
	"author": {
		"name": "Ailton Loures",
		"email": "dev.ailtonloures@proton.me"
	},
	"type": "module",
	"main": "src/main.js",
	"scripts": {
		"start": "electron-forge start --enable-logging",
		"make:icons": "electron-icon-maker --input=./assets/icon.png --output=./assets",
		"test": "vitest run --sequence.shuffle.files",
		"lint": "eslint --fix",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish"
	},
	"dependencies": {
		"electron-squirrel-startup": "^1.0.1",
		"electron-store": "^10.0.0",
		"lokijs": "^1.5.12"
	},
	"devDependencies": {
		"@electron-forge/cli": "^7.4.0",
		"@electron-forge/maker-deb": "^7.4.0",
		"@electron-forge/maker-rpm": "^7.4.0",
		"@electron-forge/maker-squirrel": "^7.4.0",
		"@electron-forge/maker-zip": "^7.4.0",
		"@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
		"@electron-forge/plugin-fuses": "^7.4.0",
		"@electron/fuses": "^1.8.0",
		"@eslint/js": "^9.9.1",
		"electron": "32.0.1",
		"electron-icon-maker": "^0.0.5",
		"eslint": "^9.9.1",
		"eslint-config-prettier": "^9.1.0",
		"globals": "^15.9.0",
		"prettier": "^3.3.3",
		"vitest": "^2.0.5"
	}
}
